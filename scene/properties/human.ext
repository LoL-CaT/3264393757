{extender "bodykit"}
{extender "vitality"
	{Energy
		{Load 				20 		40		100 } ; equipment weight, kg
		{Update
			{stay	+15		+10		+9	 	+5  }	; fatigue per second
			{walk 	+10     +5	 	0       -10 }	; max.fatigue = 100
			{slow  	+10		+5	 	0		-10 }
			{normal	+2.5    +1		0       -20 }
			{fast 	-5		-10	    -20		-40 }
		}
		{movement_limits
			{Fast  	1 25}
			{Normal 0 0}
			{Slow  -1 -1} ; turn slow mode off
		}
	}
}
