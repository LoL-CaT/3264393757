{"gem2/bump" "bark"
	{diffuse "$/flora/african_akacia_bark"}
	{bump "$/flora/african_akacia_bark_nm"}
	{specular "$/flora/african_akacia_bark_sp"}
	{blend none}
}
{"gem2/bump_wind" "leaves"
	{diffuse "$/flora/african_akacia"}
	{bump "$/flora/african_akacia_nm"}
	{specular "$/flora/african_akacia_sp"}
	{bumpvolume "$/water/nwave02"}
	{bumpvolume_period 2}
	{bumpvolume_bumpiness 30}
	{blend test}
}
{"gem2/bump" "rootx"
	{diffuse "$/flora/root_001"}
	{bump "$/flora/root_001_nm"}
	{specular "$/flora/root_001_sp"}
	{blend test}
}
