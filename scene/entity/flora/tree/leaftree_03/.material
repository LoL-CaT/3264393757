{"gem2/bump" "bark"
	{diffuse "$/flora/leaftree_bark_01"}
	{bump "$/flora/leaftree_bark_01_nm"}
	{specular "$/flora/leaftree_bark_01_sp"}
	{blend none}
}
{"gem2/bump_wind" "branches"
	{diffuse "$/flora/leaftree_branches_01"}
	{bump "$/flora/leaftree_branches_01_nm"}
	{specular "$/flora/leaftree_branches_01_sp"}
	{bumpvolume "$/water/nwave02"}
	{bumpvolume_period 2}
	{bumpvolume_bumpiness 80}
	{bumpvolume_tiles 2}
	{blend test}
}
{"gem2/bump" "root"
	{diffuse "$/flora/root_001"}
	{bump "$/flora/root_001_nm"}
	{specular "$/flora/root_001_sp"}
	{blend test}
}
