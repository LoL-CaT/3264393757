{"gem2/bump" "branch_bare"
	{diffuse "$/flora/branch_birch"}
	{bump "$/flora/branch_001_g2_nm"}
	{specular "$/flora/branch_001_g2_sp"}
	{blend test}
}
{"gem2/bump_wind" "leaves1"
	{diffuse "$/flora/branch_001_g2"}
	{bump "$/flora/branch_001_g2_nm"}
	{specular "$/flora/branch_001_g2_sp"}
	{bumpvolume "$/water/nwave02"}
	{bumpvolume_period 2}
	{bumpvolume_bumpiness 80}
	{blend test}
}
{"gem2/bump" "root"
	{diffuse "$/flora/root_001"}
	{bump "$/flora/root_001_nm"}
	{specular "$/flora/root_001_sp"}
	{blend test}
}
{"gem2/bump" "trunk"
	{diffuse "$/flora/leaved2_trunk"}
	{bump "$/flora/leaved1_trunk_nm"}
	{specular "$/flora/leaved1_trunk_sp"}
	{blend none}
}
