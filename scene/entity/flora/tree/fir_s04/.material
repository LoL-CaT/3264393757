{"gem2/bump" "bark"
	{diffuse "$/flora/fir_bark_s01"}
	{bump "$/flora/fir_bark_s01_nm"}
	{specular "$/flora/fir_bark_s01_sp"}
	{blend none}
}
{"gem2/bump_wind" "branch"
	{diffuse "$/flora/fir_branches_s01"}
	{bump "$/flora/fir_branches_s01_nm"}
	{specular "$/flora/fir_branches_s01_sp"}
	{bumpvolume "$/water/nwave02"}
	{bumpvolume_period 2}
	{bumpvolume_bumpiness 30}
	{bumpvolume_tiles 0.5}
	{blend test}
}
{"gem2/bump" "root"
	{diffuse "$/flora/root_001"}
	{bump "$/flora/root_001_nm"}
	{specular "$/flora/root_001_sp"}
	{blend test}
}
