{"gem2/bump" "body"
	{diffuse "$/entourage/bonfire"}
	{bump "$/entourage/bonfire_nm"}
	{specular "$/entourage/bonfire_sp"}
	{blend none}
}
{"pbr/standard" "cauldron"
	{diffuse "$/entourage/cauldron"}
	{bump "$/entourage/cauldron_nm"}
	{ao "$/entourage/cauldron_ao"}
	{roughness "$/entourage/cauldron_rg"}
	{metallic "$/entourage/cauldron_mt"}
	{metallic_rate 1}
	{blend none}
}
{"gem2/bump" "light"
	{diffuse "$/entourage/bonfire_light"}
	{bump "$/entourage/bonfire_nm"}
	{specular "$/entourage/bonfire_sp"}
	{blend add}
}
{"pbr/standard" "water"
	{diffuse "$/entourage/cauldron_water"}
	{bump "$/entourage/cauldron_water_nm"}
	{ao "$/pbr/0_ao"}
	{roughness "$/entourage/cauldron_water_rg"}
	{metallic "$/entourage/cauldron_water_mt"}
	{blend blend}
}
