	}

(define "mgun_place"
		{place "%name"
			{type "hull"}
			{weapon "dt" filled}
;			{foresight "%foresight"}
			{linkBone "%linkbone"}
			{gunner "%gunner"}
			{charger "%gunner"}
			("abm_mgun")
;			{basic}
		}
)
	{Weaponry
		("restore_ik_time")
		("mgun_place" name(mgun1) linkbone(mgun1) gunner(gunner1))
