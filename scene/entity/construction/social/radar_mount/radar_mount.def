{actor
	(include "/properties/construction.inc")
	{props "big" "steel" "one" "staged" "construction" "parts" "no_destroyed_stamp" "no_disable_volumes" }
	("armor_heavy" args 0.8)
	{obstacleID "unbreakable"}

	{Extension "radar_mount.mdl"}

	{placer
		{place "1" {attach  hide_stand} 		{linkbone "attach1"}}
	}
	{Boarder
		{door "emit" {queue 0}}
