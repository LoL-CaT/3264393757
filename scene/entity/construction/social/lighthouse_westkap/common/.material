{"gem2/bump_simple" "lighthouse.mtl"
	{diffuse "$/construction/lighthouse_wk"}
	{bump "$/construction/lighthouse_bmp"}
	{blend none}
}
{"gem2/bump_simple" "lighthouse_fence.mtl"
	{diffuse "$/decor/lh_wk_fnc"}
	{bump "$/decor/lh_wk_fnc_bmp"}
	{blend blend}
}
{"gem2/simple" "lighthouse_light.mtl"
	{diffuse "$/window & door/lh_light"}
	{blend add}
}
{"gem2/multiply" "lighthouse_wall.mtl"
	{diffuse "$/construction/lighthouse_wk"}
	{diffuse1 "$/model/lh_wk_lm"}
	{blend none}
}
{"gem2/simple" "lighthouse_wind.mtl"
	{diffuse "$/window & door/lh_wk_wnd"}
	{amount 0.6}
	{blend blend}
}
