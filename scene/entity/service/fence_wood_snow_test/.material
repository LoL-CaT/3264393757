{"pbr/standard_lightmap" "bark"
	{diffuse "$/wood/wood_old"}
	{bump "$/wood/wood_old_nm"}
	{roughness "$/wood/wood_old_rg"}
	{ao "$/pbr/0_ao"}
	{metallic "$/pbr/0_met"}
	{lightmap "fence_wood_r_lm"}
	{blend none}
}
{"pbr/standard_lightmap_layered" "planks"
	{diffuse "wood_planks_grey"}
	{bump "wood_planks_grey_nm"}
	{roughness "wood_planks_grey_rg"}
	{ao "wood_planks_grey_ao"}
	{metallic "$/pbr/0_met"}
	{lightmap "fence_wood_r_lm"}

	{mask "snow_mask"}
	{mask_bump "snow_test_nm"}
	{mask_diffuse "snow_test"}
	{mask_roughness "snow_test_rg"}
	{mask_metallic "$/pbr/0_met"}
	{mask_ao "$/pbr/0_ao"}

	{blend none}
}
