	(define "v_bodyback"
	{volume "bodyback%0_"
		{component "bodyback"}
		{tags "body" "body_armorback"}
	}
	)
	("v_bodyback" args 01)
	("v_bodyback" args 02)
	("v_bodyback" args 03)
	("v_bodyback" args 04)
	("v_bodyback" args 05)
	(define "v_cabin"
	{volume "cabin%0"
			{component "cabin"}
			{tags "cabin" "cabin_armor"}
	}
	)
	{volume "cabin01"
			{able {holed}}
			{component "cabin"}
			{tags "cabin" "cabin_armor"}
	}
	("v_cabin" args 02)
	{volume "doorl"
			{component "cabin"}
			{tags "cabin" "cabin_armor"}
			{able {holed}}
	}
	{volume "doorr"
			{component "cabin"}
			{tags "cabin" "cabin_armor"}
			{able {holed}}
	}
	
	("arena_armor_car" engine(5) body(5) cabin(5) shield(5) hp(100))	
	{armor
		("module_body_light"	n(back)	body(15))
		("module_body_light"	n(01)	body(30))
		("module_other" 		t(1))
	}
	
	{boarder
		{anm "passenger"
			{forward  {begin  "board_body"} {end "passenger_car_small"}}
			{reverse  {end  "emit_body" 1}}
		}
	}
  
	("crew_human" place(seat1)	bone(seat01)	door(emit3) animation(passenger))
	("crew_human" place(seat2)	bone(seat02)	door(emit4)	animation(passenger))
	("crew_human" place(seat3)	bone(seat03)	door(emit3)	animation(passenger))
	("crew_human" place(seat4)	bone(seat04)	door(emit4)	animation(passenger))
	("crew_human" place(seat5)	bone(seat05)	door(emit3) animation(passenger))
	("crew_human" place(seat6)	bone(seat06)	door(emit4)	animation(passenger))
	("crew_human" place(seat7)	bone(seat07)	door(emit3)	animation(passenger))
	("crew_human" place(seat8)	bone(seat08)	door(emit4)	animation(passenger))
	("crew_human" place(seat9)	bone(seat09)	door(emit3)	animation(passenger))
	("crew_human" place(seat10)	bone(seat10)	door(emit4)	animation(passenger))
	
	{placer
		{place "driver" 	{visor "main"}{group "crew"}}
		{place "commander"	{visor "main"}{group "crew"}}
	}
	
 
  ("wheel" args "wheelright1")
  ("wheel" args "wheelleft1")
  ("wheel" args "wheelright2")
  ("wheel" args "wheelleft2")

	("components_car" 
		repair_body(20)
		repair_engine(10)
		repair_wheel(5)
	)
  
	{extender "inventory"
		{box
			{size 6 10}
			{clear}
			{item "repair_kit" 2 {cell 0 0}}
			{item "can" filling "fuel" 60 {cell 2 0}}
			{item "antipersonnel" "mine" 5 {cell 4 0}}
			{item "bandage" 10 {cell 2 1}}
			{item "bandage" 10 {cell 4 1}}
