			{item "mgun" "ammo" 500 {cell 2 9}}
			{item "dynamite" 2 {cell 4 5}}
			{item "sapper_shovel" 2}			
		}
	}
	
	{mass 3500}
	{Chassis
		{Locomotion ("locomotion")
			{MaxSpeed	       50}
			{maxSpeedAtMaxTurn 50}
			{StartTime	       1}
			{TurnTime	        1}
			{TurnStart        0.5}
			{BrakeTime        0.5}
			{StopTime         0.5}
			{TurnRadius         3}
			{TurnStop         0.1}
			{Gears      0.5  1}
			{RearGears       0.9}
		}
		("fuelbag"
			volume(319)
			rate(50)
			remain(319)
		)
		("car_trace" fx("tracewheel02") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel02") step(0.09) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		{MaxTrailerMass 3750}
	}
	("chassis_add" speed(80))
	{Extension "opel_blitz_highback.mdl"}
    ("animation_resume")

	{extension "sensor.ext"}
	{extension "camouflage.ext"}
