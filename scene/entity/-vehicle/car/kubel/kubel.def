{game_entity
	(include "/properties/car.ext")
	(include "/properties/shadow/tank.inc"  scale(0.9))
	(include "/properties/selection/vehicle.inc"  scale(1.1))
	{mass 1160}
	{props "vehicle" "car" "wheeled" "unarmed" "texmod_w_cf_cd"}
	{able "-emit_passangers_on_enemy"}
	{PatherId "vehicle.small"}

	("components_car" 
		repair_body(20)
		repair_engine(10)
		repair_wheel(5)
	)


	{volume "shield"
		{thickness 0.3}
	}
	{component "shield"	
		{RepairTime 20}
	}
	("v_body" n(01) t())
	("v_body" n(02) t())
	("v_body" n(03) t())
	("v_body" n(04) t(1))
	("v_body" n(05) t(1))
	{volume "doorl1"
			{component "cabin"}
			{tags "cabin" "cabin_armor"}
			{able {holed}}
	}
	{volume "doorr1"
			{component "cabin"}
			{tags "cabin" "cabin_armor"}
			{able {holed}}
	}
	{volume "doorl2"
			{component "cabin"}
			{tags "cabin" "cabin_armor"}
			{able {holed}}
	}
	{volume "doorr2"
			{component "cabin"}
			{tags "cabin" "cabin_armor"}
			{able {holed}}
	}
	("arena_armor_car" engine(5) body(5) cabin(4) shield(1) hp(235))
	{armor
		("module_body_light"	n(1)	body(5))
		("module_other" 		t(1))
	}
	("wheel_custom" v(wheelr1)	c(wheelright1))	
	("wheel_custom" v(wheell1)	c(wheelleft1))
	("wheel_custom" v(wheelr2)	c(wheelright2))	
	("wheel_custom" v(wheell2)	c(wheelleft2))
	
	{extender "shield"
		{on}
	}

	{extender "enumerator"}

	("crew_2_human")
 	("crew_human" place(seat1) bone(seat01) door(emit3) animation(passenger1))
	("crew_human" place(seat2) bone(seat02) door(emit4) animation(passenger2))

	{boarder
		{anm "driver"
			{forward	{begin	"board_Kubel_driver"} {end "driver_car_small"} {base "doorleft"}}
			{reverse 	{end	"board_Kubel_driver" -1} {base "doorleft" -1}}
		}
		{anm "commander"
			{forward	{begin	"board_Kubel_commander"} {end "passenger_car_small"} {base "doorright"}}
			{reverse 	{end	"board_Kubel_commander" -1} {base "doorright" -1}}
		}
		{anm "passenger1"
			{forward	{begin	"board_Kubel_leftpassenger"} {end "passenger_car_small"} {base "doorleft1"}}
			{reverse 	{end	"board_Kubel_leftpassenger" -1} {base "doorleft1" -1}}
		}
		{anm "passenger2"
			{forward	{begin	"board_Kubel_rightpassenger"} {end "passenger_car_small"} {base "doorright1"}}
			{reverse 	{end	"board_Kubel_rightpassenger" -1} {base "doorright1" -1}}
		}
	}
	{placer
		{place "driver" 	{visor "main"}{group "crew"}}
		{place "commander"	{visor "main"}{group "crew"}}
	}

	{extender "inventory"
		{Box
		{clear}
		{item "repair_kit"}
		{item "can" filling "fuel" 60}
		{item "sapper_shovel" 2}		
		}
	}
	{Chassis
		{Locomotion ("locomotion")
			{MaxSpeed	40}
			{StartTime	2}		
			{maxSpeedAtMaxTurn 25}
			{BrakeTime	1}		
			{StopTime	1}
			{TurnRadius	4}
			{Gears		0.1 0.5 0.8 1.0}
			{RearGears	0.5}
			{SteerOn	20}
			{SteerOff	40}
			{SplineRadius 1}		
		}
		("fuelbag"
			volume(438)
			rate(50)
			remain(438)
		)
		{MaxTrailerMass 587.5}	
	}	
	("chassis_add" speed(80))
	{Extension "kubel.mdl"}
    ("animation_resume")
	{extension "sensor.ext"}
	{texmod ""}
}