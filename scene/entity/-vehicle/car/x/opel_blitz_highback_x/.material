{"gem2/bump" "body"
	{diffuse "$/model/x/opel_blitz/opel_blitz_x"}
	{bump "$/model/opel_blitz/opel_blitz_nm"}
	{specular "$/model/x/opel_blitz/opel_blitz_sp_x"}
	{blend none}
}
{"gem2/specular" "glass"
	{diffuse "$/model/opel_blitz/opel_blitz_glass"}
	{specular "$/model/opel_blitz/opel_blitz_glass_sp"}
	{blend blend}
}
{"gem2/bump" "wheel"
	{diffuse "$/model/wheels/opel_blitz_wheel_x"}
	{bump "$/model/wheels/opel_blitz_wheel_nm"}
	{specular "$/model/wheels/opel_blitz_wheel_sp_x"}
	{blend none}
}
