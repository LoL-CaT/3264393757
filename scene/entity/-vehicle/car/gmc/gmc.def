{entity
	(include "/properties/car.ext")
	(include "/properties/shadow/tank.inc"  scale(1.0))
	(include "/properties/selection/vehicle.inc"  scale(1.1))
	{collider "truck"}
	{props "vehicle" "car" "truck" "wheeled" "vision_lev03" "unarmed" "texmod_w_cf_cd"}

	("v_body" n(01) t())
	("v_body" n(02) t())
	("v_body" n(03) t())
	("v_body" n(04) t())
	("v_body" n(05) t())
	("v_body" n(06) t())
	("v_body" n(07) t())
	("v_body" n(08) t())
	("v_body" n(09) t())
	(define "v_cabin"
	{volume "cabin%0"
			{component "cabin"}
			{tags "cabin" "cabin_armor"}
	}
	)
	("v_cabin" args 01)
	("v_cabin" args 02)
	("v_cabin" args 03)
	("v_cabin" args 04)
	("v_cabin" args 05)
	("v_cabin" args 06)
	("v_cabin" args 07)
	("v_cabin" args 08)
	("v_engine" n(01) t())
	("v_engine" n(02) t())
	("v_engine" n(03) t())
	("v_engine" n(04) t())
	("v_engine" n(05) t())
	{volume "window01"
		{tags "glass"}
	}
	{volume "window02"
		{tags "glass"}
	}

	("arena_armor_car" engine(7) body(7) cabin(5) shield(5) hp(100))

	("crew_2_human")
	("board_animation" place(driver) board_begin(board_bigcar) board_end(driver_car_small))
	("board_animation" place(commander) board_begin(board_bigcar) board_end(passenger_car_small))
	{boarder
		{anm "passenger"
			{forward	{begin	"board_dodge_bodyback"} {end "passenger_car_small"} {base "cover"}}
			{reverse 	{end	"emit_body" 1} {base "cover"}}
		}
	}
	("crew_human" place(seat1)	bone(seat011)	door(emit3) animation(passenger))
	("crew_human" place(seat2)	bone(seat012)	door(emit4)	animation(passenger))
	("crew_human" place(seat3)	bone(seat013)	door(emit3)	animation(passenger))
	("crew_human" place(seat4)	bone(seat014)	door(emit4)	animation(passenger))
	("crew_human" place(seat5)	bone(seat015)	door(emit3)	animation(passenger))
	("crew_human" place(seat6)	bone(seat016)	door(emit4)	animation(passenger))
	("crew_human" place(seat7)	bone(seat017)	door(emit3)	animation(passenger))
	("crew_human" place(seat8)	bone(seat018)	door(emit4)	animation(passenger))
	("crew_human" place(seat9)	bone(seat019)	door(emit3)	animation(passenger))
	("crew_human" place(seat10) bone(seat020)	door(emit4)	animation(passenger))

	{extender "enumerator"
		{group "usa"}
	}
	{extender "detail"
		{incompatible "1" "2"}
		{incompatible "2" "1"}
	}
	("seat_inside")
	{placer
		{place "driver" 	{visor "main"}{group "crew"}}
		{place "commander"	{visor "main"}{group "crew"}}
	}
	

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")
	("wheel" args "wheelright3")
	("wheel" args "wheelleft3")

	{volume "shield_front"
		{able {nowounddecal}}
		{thickness 1}
	}
	{component "shield_front"
		{RepairTime 10}
	}
	{mass 4500}
	{Chassis
		{Locomotion ("locomotion")
			{MaxSpeed	       50}
			{maxSpeedAtMaxTurn 50}
			{StartTime	       1}
			{TurnTime	        1}
			{TurnStart        0.5}
			{BrakeTime        0.5}
			{StopTime         0.5}
			{TurnRadius         3}
			{TurnStop         0.1}
			{Gears      0.5  1}
			{RearGears       0.9}
		}
		("fuelbag"
			volume(475)
			rate(50)
			remain(475)
		)
		("car_trace" fx("tracewheel01") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel01") step(0.09) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		{MaxTrailerMass 2275}
	}	
	("chassis_add" speed(72))
	{extender "shield"
			{on}
	}

	{Extension "gmc.mdl"}
	("animation_resume")

	{extender "inventory"
		{box
			{size 6 10}
			{clear}
			{item "repair_kit" 2 {cell 0 0}}
			{item "can" filling "fuel" 60 {cell 2 0}}
			{item "sapper_shovel" 2}	
			{item "bullet37 ap usa m1" 150}
			{item "bullet37 fg usa m1" 150}
			{item "ammo bullet57 ap eng std" 25}
			{item "ammo bullet57 fg eng std" 60}
			{item "bullet76 ap usa std" 50}
			{item "bullet76 fg usa std" 50}			
		}
	}
	{extension "sensor.ext"}
	{extension "camouflage.ext"}
}