{game_entity
	(include "/properties/car.ext")
	(include "/properties/shadow/tank.inc"  scale(0.9))
	(include "/properties/selection/vehicle.inc"  scale(1.1))
	{mass 2500}
	{props "vehicle" "car" "wheeled" "turn_over" "unarmed" "dodge"}
	{able "-emit_passangers_on_enemy"}
	{PatherId "vehicle.small"}

	("sandbags_m4")

	{extender "enumerator"
		{group "usa"}
	}
	{extender "resuscitation"
		{radius 22}
	}	

	{volume "sticky"
		{tags "grenade_fg_target"}
		{able clear {contact} {touch}}
		{component "sticky_body"}
	}
	{component "sticky_body"}

;	("v_body" n(01) t())
	("v_body" n(02) t())
	("v_body" n(03) t())
	("v_body" n(04) t())
	("v_body" n(05) t())
	("v_engine" n(01) t())

	("v_window" n(01))
	("v_window" n(02))

	("v_cabin" n(01))
	("v_cabin" n(02))
	("v_cabin" n(03))
	("arena_armor_car" engine(5) body(5) cabin(4) shield(1) hp(215))
	{armor
		("module_other" 		t(5))
	}

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")

	("crew_2_human")

	("board_animation"
		place(driver)
		board_begin(board_willis_driver)
		board_end(driver_car_small)
		base(doorleft)
	)

	("board_animation"
		place(commander)
		board_begin(board_willis)
		board_end(passenger_car_small)
		base(doorright)
	)
	
	{placer
		{place "driver"		{group "crew"} {visor "main"}}
		{place "commander"	{group "crew"} {visor "main"}}
	}

	{extender "inventory"
		{Box
		{clear}
			{item "repair_kit"}
			{item "med_point_kit"}
			{item "can" filling "fuel" 60}
			{item "sapper_shovel" 2}
			{item "sandbag_kit2"}
			{item "medkit" {cell 4 8}}
			{item "bandage" 5 {cell 2 9}}
			{item "bandage" 5 {cell 0 9}}
			{item "bandage" 5 {cell 0 8}}
			{item "bandage" 5 {cell 2 8}}
			{item "bandage" 5 {cell 0 7}}
			{item "bandage" 5 {cell 2 7}}
			{item "bandage" 5 {cell 4 7}}
			{item "bandage" 5 {cell 0 6}}
			{item "bandage" 5 {cell 2 6}}
			{item "bandage" 5 {cell 4 6}}
			{item "bandage" 5 {cell 0 5}}
			{item "bandage" 5 {cell 2 5}}
			{item "bandage" 5 {cell 4 5}}
			{item "bandage" 5 {cell 0 4}}
			{item "bandage" 5 {cell 2 4}}
			{item "bandage" 5 {cell 4 4}}
			{item "bandage" 5 {cell 3 3}}
			{item "bandage" 5 {cell 3 2}}			
		}
	}
	{extender "detail"
	{random 0}
    }
	{Chassis
		{Locomotion ("locomotion")
				{MaxSpeed	40}
				{StartTime	2}
				{maxSpeedAtMaxTurn 25}
				{BrakeTime	1}
				{StopTime	1}
				{TurnRadius	4}
				{Gears		0.1 0.5 0.8 1.0}
				{RearGears	0.5}
				{SteerOn	20}
				{SteerOff	40}
				{SplineRadius 1}
			}
		("fuelbag"
			volume(381)
			rate(50)
			remain(381)
		)				
		{MaxTrailerMass 1975}
	}	
	("chassis_add" speed(87))
	{Extension "dodge_med.mdl"}
    ("animation_resume")
	{extension "sensor.ext"}
	("components_car" 
		repair_body(20)
		repair_engine(10)
		repair_wheel(5)
	)
	{extension "camouflage.ext"}
}