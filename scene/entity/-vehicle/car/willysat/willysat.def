	("arena_armor_car" engine(5) body(5) cabin(4) shield(5) hp(215))
	{armor
		("module_other" 		t(5))
	}
	("wheel_custom" v(wheelr1)	c(wheelright1))	
	("wheel_custom" v(wheell1)	c(wheelleft1))
	("wheel_custom" v(wheelr2)	c(wheelright2))	
	("wheel_custom" v(wheell2)	c(wheelleft2))
	("components_car" 
		repair_body(20)
		repair_engine(10)
		repair_wheel(5)
	)

	("board_animation"
		place(driver)
		board_begin(board_willysat_driver)
		board_end(driver_car_small)
		base(doorleft)
	)
	{boarder
		{anm "gunner"
			{forward {begin	"board_LowTank_stand"} {end "passenger_car_small"}}
			{reverse {end "emit_tank_1_hold" 1}}
		}
	}

	("crew_human" place(driver)	bone(driver) door(emit1) animation(driver))
	("crew_human" place(gunner)	bone(gunner) door(emit2) animation(gunner))

	{placer
		{place "driver" {group "crew"} {visor "driver"}}
		{place "gunner" {group "crew"} {visor "main"} {turnoff {targetable 0}}}
	}
	

	{extender "inventory"
		{Box
			{size 5 6}
			{clear}
			{item "repair_kit" {cell 0 0}}
			{item "can" filling "fuel" 60}			
			{item  "bazooka ammo" 20}
		}
	}
	{Weaponry
		{place "bazooka"
			{weapon "doublebazooka weapon" filled}
			{Barrels 2}
			{charger "gunner"}
			("abm_gun")
		}
	}	
	{mass 1160}
	{Chassis
		{Locomotion ("locomotion")
			{MaxSpeed	40}
			{StartTime	2}
			{maxSpeedAtMaxTurn 25}
			{BrakeTime	1}
			{StopTime	1}
			{TurnRadius	4}
			{Gears		0.1 0.5 0.8 1.0}
			{RearGears	0.5}
			{SteerOn	20}
			{SteerOff	40}
			{SplineRadius 1}
		}
		("fuelbag"
			volume(444)
			rate(50)
			remain(444)
		)
		{MaxTrailerMass 1675}
		("car_trace" fx("tracewheel01a") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel01a") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
	}
	("chassis_add" speed(105))
	{Extension "willysat.mdl"}
    ("animation_resume")
	{bone "gun_rot"
		{limits}
		{speed 0.035}
	}
	{bone "gun_rot1"
		{limits -35 10}
		{speed 0.035}
	}
	{extension "sensor.ext"}
}