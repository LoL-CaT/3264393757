{entity
	(include "/properties/car.ext")
	(include "/properties/shadow/tank.inc"  scale(0.8))
	(include "/properties/selection/vehicle.inc"  scale(1.1))
	{props "vehicle" "car" "wheeled" "turn_over" "vision_lev01" "spotter" "texmod_w_cf_cd" "one_hand_mgun_anm"}
	{PatherId "vehicle.small"}

	("components_car" 
		repair_body(20)
		repair_engine(10)
		repair_wheel(5)
	)
	
	{extender "enumerator"}

	("v_body" n(01) t())
	("v_body" n(02) t())
	("v_body" n(03) t())
	{volume "doorl1"
			{component "cabin"}
			{tags "cabin" "cabin_armor"}
			{able {holed}}
	}
	{volume "doorr1"
			{component "cabin"}
			{tags "cabin" "cabin_armor"}
			{able {holed}}
	}
	{volume "doorl2"
			{component "cabin"}
			{tags "cabin" "cabin_armor"}
			{able {holed}}
	}
	{volume "doorr2"
			{component "cabin"}
			{tags "cabin" "cabin_armor"}
			{able {holed}}
	}
	("arena_armor_car" engine(5) body(5) cabin(4) shield(1) hp(235))
	{armor
		("module_other" 		t(1))
	}

	("wheel_custom" v(wheelr1)	c(wheelright1))
	("wheel_custom" v(wheell1)	c(wheelleft1))
	("wheel_custom" v(wheelr2)	c(wheelright2))
	("wheel_custom" v(wheell2)	c(wheelleft2))

	("crew_2_human")
	("crew_human" place(gunner) bone(gunner) door(emit3) animation(gunner))
	{placer
		{place "driver"		{group "driver"} {visor "driver"}}
		{place "commander"	{group "driver"} {visor "main"}}
		{place "gunner"		{group "driver"} {visor "main"} {turnoff {targetable 0}}}
	}

	("board_animation" place(driver) board_begin(board_kubel_driver) board_end(driver_car_small) base(doorleft))
	("board_animation" place(commander) board_begin(board_Kubel_commander) board_end(passenger_car_small) base(doorright))
	{boarder
		{anm "gunner"
			{forward {begin "board_LowTank_stand"} {end "vehicle_mgun_1hand"}}
			{reverse {end "emit_tank_1_hold" 1}}
		}
	}

	{extender "inventory"
		{weapon "mgun"
			{mask "weapon mgun"}
		}
		{Box
			{clear}
			{item "repair_kit" {cell 0 0}}
			{item "can" filling "fuel" 60}	
			{item  "ammo mgun" 1200}
			{item "mg34_vh_car" filled {user "mgun"}}
		}
	}
	{Weaponry
		{place "mgun"
			{charger "gunner"}
			{LinkBone "mgun1"}
			{LinkAnimation "off"}
			("abm_mgun")
			{basic}
		}
	}
	
	{mass 1160}
	{Chassis
		{Locomotion ("locomotion")
			{MaxSpeed	40}
			{StartTime	2}		
			{maxSpeedAtMaxTurn 25}
			{BrakeTime	1}		
			{StopTime	1}
				{TurnRadius	4}
			{Gears		0.1 0.5 0.8 1.0}
			{RearGears	0.5}
			{SteerOn	20}
			{SteerOff	40}
			{SplineRadius 1}
		}
		("fuelbag"
			volume(438)
			rate(50)
			remain(438)
		)
		{MaxTrailerMass 587.5}			
	}	
	("chassis_add" speed(80))
	{Extension "kubel_mgun.mdl"}
    ("animation_resume")

	{bone "gun_rot"
		{speed 0.05}
        {limits -27 50}
	}
	{bone "gun_vert"
		{limits -25 20}
		{speed 0.025}
	}

	{extension "sensor.ext"}
	{extension "crew.ext"}
}