{game_entity
	(include "/properties/car.ext")
  	(include "/properties/shadow/tank.inc"  scale(1.0))
 	(include "/properties/selection/vehicle.inc"  scale(1.1))
 	{collider "truck"}
  	{props "vehicle" "car" "wheeled" "windowed" "truck" "vision_lev03" "unarmed" "texmod_w_cf_cd"}
	{able "-emit_passangers_on_enemy"}

	{extender "resuscitation"
		{radius 22}
	}
			
	("crew_2_human")
	("board_animation"
    place(driver)
    board_begin(board_big-midcar_driver)
    board_end(driver_car_small)
    base(doorleft)
	)
	("board_animation"
    place(commander)
    board_begin(board_big-midcar)
    board_end(passenger_car_small)
    base(doorright)
	)

	("v_body" n(01) t(01))
	("v_body" n(02) t())
	("v_body" n(03) t())
	("v_body" n(04) t())
	("v_body" n(05) t())
	("v_body" n(06) t())
	("v_body" n(07) t())
	("v_body" n(08) t())

	(define "v_bodyback"
	{volume "bodyback%0"
		{component "bodyback"}
		{tags "body" "body_armorback"}
	}
	)
	("v_bodyback" args 01)
	("v_bodyback" args 02)
	(define "v_board"
	{volume "board%0"
		{component "bodyback"}
		{tags "body" "body_armorback"}
	}
	)
	("v_board" args l)
	("v_board" args r)
	("v_board" args _back)
	
	(define "v_cabin"
	{volume "cabin%0"
			{component "cabin"}
			{tags "cabin" "cabin_armor"}
	}
	)
	("v_cabin" args 01)
	("v_cabin" args 02)
	("v_cabin" args 03)
	("v_cabin" args 04)
	{volume "doorl"
			{component "cabin"}
			{tags "cabin" "cabin_armor"}
			{able {holed}}
	}
	{volume "doorr"
			{component "cabin"}
			{tags "cabin" "cabin_armor"}
			{able {holed}}
	}
	("arena_armor_car" engine(1) body(5) cabin(1) shield(1) hp(100))	
	{armor
		("module_body_light"	n(back)	body(15))
		("module_body_light"	n(01)	body(30))
		("module_other" 		t(1))
	}
	
	{placer
		{place "driver" 	{visor "main"}{group "crew"}}
		{place "commander"	{visor "main"}{group "crew"}}
	}
	
  
	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")

	("components_car" 
		repair_body(20)
		repair_engine(10)
		repair_wheel(5)
	)
  
	{extender "inventory"
		{box
			{clear}
			{item "repair_kit" 2 }
			{item "med_point_kit"}
			{item "can" filling "fuel" 60}
			{item "sapper_shovel" 2}
			{item "sandbag_kit2"}
			{item "medkit" {cell 4 8}}
			{item "bandage" 5 {cell 2 9}}
			{item "bandage" 5 {cell 0 9}}
			{item "bandage" 5 {cell 0 8}}
			{item "bandage" 5 {cell 2 8}}
			{item "bandage" 5 {cell 0 7}}
			{item "bandage" 5 {cell 2 7}}
			{item "bandage" 5 {cell 4 7}}
			{item "bandage" 5 {cell 0 6}}
			{item "bandage" 5 {cell 2 6}}
			{item "bandage" 5 {cell 4 6}}
			{item "bandage" 5 {cell 0 5}}
			{item "bandage" 5 {cell 2 5}}
			{item "bandage" 5 {cell 4 5}}
			{item "bandage" 5 {cell 0 4}}
			{item "bandage" 5 {cell 2 4}}
			{item "bandage" 5 {cell 4 4}}
			{item "bandage" 5 {cell 3 3}}
			{item "bandage" 5 {cell 3 2}}			
		}
	}
  
	{mass 1750}
	{Chassis
		{Locomotion ("locomotion")
			{MaxSpeed	       40}		
			{maxSpeedAtMaxTurn 45}
			{StartTime	       1}
			{TurnTime	        1}
			{TurnStart        0.5} 
			{BrakeTime        0.5}
			{StopTime         0.5} 
			{TurnRadius         3} 
			{TurnStop         0.1} 
			{Gears      0.5  1}
			{RearGears       0.9}
		}
		("fuelbag"
			volume(213)
			rate(50)
			remain(213)
		)
		("car_trace" fx("tracewheel02") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel02") step(0.09) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		{MaxTrailerMass 2500}			
	}
	("chassis_add" speed(70))
	{Extension "gaz_aa_med.mdl"}
    ("animation_resume")
	{extender "enumerator"}
	{extension "sensor.ext"}
	{extension "camouflage.ext"}
	{extender "bodykit"
		{"round_sandbag"
			{delay 10}
			{"sandbags_flaktruck" "basis"}		
		}	
	}	
}