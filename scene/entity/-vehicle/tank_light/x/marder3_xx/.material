{"gem2/bump_specular_lightmap" "body"
	{diffuse "$/model/x/marder3/marder3_x"}
	{bump "$/model/marder3/marder3_nm"}
	{specular "$/model/x/marder3/marder3_sp_x"}
	{lightmap "$/model/x/sd_kfz_251/sdkfz_251_lm"}
	{blend none}
}
{"gem2/bump_specular_lightmap" "body_alpha"
	{diffuse "$/model/x/marder3/marder3_x"}
	{bump "$/model/marder3/marder3_nm"}
	{specular "$/model/x/marder3/marder3_sp_x"}
	{lightmap "$/model/x/sd_kfz_251/sdkfz_251_lm"}
	{blend test}
}
{"gem2/bump" "tracks"
	{diffuse "$/model/track/pz_kpfw_38_track"}
	{bump "$/model/track/pz_kpfw_38_track_nm"}
	{specular "$/model/track/pz_kpfw_38_track_sp"}
	{blend test}
}
