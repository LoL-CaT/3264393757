	}
	
	{component "engine"	
		{RepairTime 10}
	}
	{component "turret"
		{RepairTime 20}
	}
	{component "gun"
		{RepairTime 15}
	}

	{boarder
		{anm "driver"
			{forward	{begin	"board_t34_driver"} {end "seat_gunner_stand"} {base "open_driver"}}
			{reverse 	{end	"emit_t34_hold"} {base "close_driver"}}
		}
		{anm "charger"
			{forward	{begin	"board_HighArmor"} {end "zenit_pointer_right"}}
			{reverse 	{end	"emit_tank_2"}}
		}
		{anm "gunner"
			{forward	{begin	"board_HighArmor"} {end "zenit_pointer_right"}}
			{reverse 	{end	"emit_tank_2"}}
		}
		{anm "commander"
			{forward	{begin	"board_HighArmor"} {end "seat_armor"}}
			{reverse 	{end	"emit_tank_2"}}
		}
		{anm "armor"
			{forward	{begin	"board_lowArmor"} 	{end "idle_onboard_l"}}
			{reverse 	{end	"emit_tank_2"}}
		}
		{anm "armor1"
			{forward	{begin	"board_lowArmor"} 	{end "idle_onboard_r"}}
			{reverse 	{end	"emit_tank_2"}}
		}
	}

	(define "crew_human"
		{placer
			{place "%place"
				{group "crew"}
				{linkbone "%place"}
				{turnoff {sensor} {shadow}}
				{visor "main"}
			}
		}
		{boarder
			{door "%door1"}
			{door "%door2"}
			{link "%door1" "%place" {anm "%place"} {forward putoff} {reverse puton}}
			{link "%door2" "%place" {anm "%place"} {forward putoff} {reverse puton}}
		}
	)

	{placer
		{place "driver" {group "crew"} {visor "driver"}}
	}
	{boarder
		{door "emit0"}
		{door "emit1"}
		{door "emit2"}
		{link "emit0" "driver" {anm "driver"} {forward putoff} {reverse puton}}
		{link "emit1" "commander" {anm "commander"} {forward putoff} {reverse puton}}
		{link "emit2" "commander" {anm "commander"} {forward putoff} {reverse puton}}
	}

	{placer
		{place "commander" {group "crew"} {visor "main"}}
	}

	("crew_human" place(gunner)	    door1(emit1) door2(emit2))
	("crew_human" place(charger)	door1(emit2) door2(emit1))
	("seat_human" place(seat_l1) bone(seat01) door(emit3)  animation(armor))
	("seat_human" place(seat_r1) bone(seat02) door(emit4)  animation(armor1))
	("seat_human" place(seat_r2) bone(seat03) door(emit5)  animation(armor1))


