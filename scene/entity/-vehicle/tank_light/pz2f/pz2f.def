	{extender "inventory"
		{box
				{item "bullet20 ap ger flak" 90}
				{item "bullet20 fg ger flak" 90}
				{item  "ammo mgun" 1800}
			{item "mg34_vh" filled {user "mgun"}}
		}
	}
	{Weaponry
		("restore_ik_time")
		{place "gun"
			{RestoreIKAfterAim}
			{weapon "20mm_kwk38" filled}
			{gunner "gunner"}
			{charger "gunner"}
			("abm_auto_cannon")
			{basic}
		}
		{place "mgun"
			{type "mgun_coaxial"}
			{gunner "commander"}
			{charger "commander"}
			("abm_mgun")
			{basic}
		}
	}


	("crew_2_human")
	("crew_human" place(commander) door1(emit1) door2(emit2) visor(main))

	{boarder
		{anm "left"
			{forward	{begin	"board_tank_left"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "right"
			{forward	{begin	"board_tank_right"}}
			{reverse 	{end	"emit_tank_1_hold"}}
		}
		{anm "armor"
			{forward	{begin	"board_LowArmor"} 	{end "idle_onboard"}}
			{reverse 	{end	"emit_tank_1"}}
		}
		{anm "armor1"
			{forward	{begin	"board_LowArmor"} 	{end "idle_onboard_r"}}
			{reverse 	{end	"emit_tank_1"}}
		}
		
	}

	("seat_human" place(seat1)   bone(seat01) door(emit3)  animation(armor))
	("seat_human" place(seat_r1) bone(seat02) door(emit4)  animation(armor1))

	("components_tank" 
		repair_body(45)
		repair_engine(21)
		repair_turret(21)
		repair_gun(12)
		repair_track(8)
	)

	{mass 9100}
	{Chassis
		("tank_trace" fx("tracks_sm") step(0.4) len(0.31))
		{Locomotion ("locomotion")
			{maxspeed   40}
			{maxSpeedAtMaxTurn 15}
			{StartTime  7}
			{TurnTime   3}
			{TurnStart  1.2}
			{TurnRadius 7} 
			{TurnStop   0.6}
			{BrakeTime  1.5}
	    		{TurnStop         0.7} 
			{Gears     0.2 0.5 1.0}
			{RearGears       0.3 0.5}
		}
		("fuelbag"
			volume(156)
			rate(50)
			remain(156)
		)
	}
	("chassis_add_light" speed(40))
	{extension "pz2f.mdl"}
    ("animation_resume")

	{bone "turret"
		("arena_turret_speed" args 0.0208)
	}
	{bone "gun_rot"
