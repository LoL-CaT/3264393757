{entity
	(include "/properties/cannon.ext")
	(include "/properties/shadow/tank.inc"  scale(0.6))
	{Extension "203_m1.mdl"}
	(include "/properties/selection/cannon.inc"  scale(1.2))
	{props "vehicle" "cannon" "heavy" "wheeled" "vision_lev05_can" "round_sandbag2" "art_point" "level05" "howitzer" "texmod_cw_cf_cd" "anm_heavy"}
	("round_sandbag2")
	("round_dot2")
	("trench_cannon_big")
	{targetSelector "cannon_howitzer_at"}		
	{targetClass "cannon_howitzer"}	
	
 	{volume "body*"
		{tags "body"}
		{component "body"}
		{thickness 30	}   
	}
	("v_gun" n(_rot))
	
	("armor_medium_hp" turret(10 10) body(10 10 10 10) hp(250))	

	{extender "inventory"
		{box
			{item "ammo bullet203 fg usa std" 12}
		}
	}
	{Weaponry
		{ManualAccuracy 1}
		{place "gun"
			{weapon "203mm_m1" filling "ammo bullet203 fg usa std" 1}
			{charger "gunner"}
			("abm_howitzer")
		}
	}

	("crew_wheeled_gun")
	("crew_4_supporter_any_set"
		place_l1(charger_l1)
		place_r1(charger_r1)
		place_l2(charger_l2)
		place_r2(charger_r2)
	)
	{boarder
		{anm "gunner"
			{forward	{end "cannon_big_gunner" 10}}
		}
		{anm "commander"
			{forward	{end "cannon_big_commander" 10}}
		}
		{anm "charger_r1"
			{forward {end "artillery_support_r" 10}}
		}
		{anm "charger_l1"
			{forward {end "artillery_support_l" 10}}
		}
		{anm "charger_r2"
			{forward {end "artillery_support_r" 10}}
		}
		{anm "charger_l2"
			{forward {end "artillery_support_l" 10}}
		}
		{anm "seat1"
			{forward {end "cannon_crew" 10}}
		}
		{anm "seat2"
			{forward {end "cannon_crew" 10}}
		}
		{anm "seat3"
			{forward {end "cannon_crew" 10}}
		}
		{anm "seat4"
			{forward {end "cannon_crew" 10}}
		}
		{anm "seat5"
			{forward {end "cannon_crew" 10}}
		}
		{anm "seat6"
			{forward {end "cannon_crew" 10}}
		}
		{anm "driver1"
		} 
		{anm "driver2"
		}
		{anm "driver_l2"
		} 
		{anm "driver_r2"
		}
	}


	{mass 14500}

	{extender "cannon"
		{attach_time 3.5}
		{animation
			{aim "cannon_big_commander_aim" "" "" "" "" ""}
			{fire  "cannon_big_commander_reaction" "cannon_big_charger_reaction" "cannon_big_support_r_reaction" "cannon_big_support_l_reaction" "cannon_big_support_l_reaction" "cannon_big_support_r_reaction"}
			{move "cannon_pak40_pusher_left" "cannon_pak40_pusher_right" "mgun_browning_driver_r" "mgun_browning_driver_l" "mgun_browning_driver_r" "mgun_browning_driver_l"}
			{reload  "cannon_big_commander_charge" "cannon_big_charger_charge" "cannon_big_support_charge_l" "cannon_big_support_charge_r" "" ""}
			{switch "close"}
		}
		{driver {workers "driver1" "driver2"}}
		{move {obstacle "close"} {workers "driver1" "driver2" "driver_l1" "driver_r1" "driver_l2" "driver_r2"}}
		{attack {obstacle "open"} {workers "gunner" "commander" "charger_l1" "charger_r1" "charger_l2" "charger_r2"}}
		{trailing {workers "seat1" "seat2" "seat3" "seat4" "seat5" "seat6"}}
	}

	{Chassis "track"
		("tank_trace_ex" fx("tracks_mid") step(0.52) len(0.7) boneleft("wheell4") boneright("wheelr4"))
		("arena_howitzer_6gunners_carrier_heavy")
    }
	{bone "gun_rot"
		{speed 0.002}
		{limits 0 64}
	}
	{bone "turret"
		{speed 0.002}
	}
	{bone "basis"
		{limits} 	
		{speed 0.001}
	}
	{extension "sensor.ext"}
	{extension "camouflage.ext"}
	{extender "bone_rotator"
		{"look_at.gun_rotator1" {bone "gun_rotator2"} {on}}
		{"look_at.gun_rotator2" {bone "gun_rotator1"} {on}}
		{"look_at.gun_rotator3" {bone "gun_rotator4"} {on}}
		{"look_at.gun_rotator4" {bone "gun_rotator3"} {on}}
	}
}