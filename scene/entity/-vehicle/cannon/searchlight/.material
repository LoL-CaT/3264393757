{"gem2/simple" "material1.mtl"
	{diffuse "$/wall/window"}
	{blend add}
	{tile}
}
{"gem2/specular" "material2.mtl"
	{diffuse "searchlight"}
	{specular "$/model/searchlight_sp"}
	{blend none}
	{tile}
}
{"gem2/envmap_mask" "material3.mtl"
	{diffuse "searchlight"}
	{envmap "$/environment/envmap_glass01"}
	{mask "searchlight"}
	{blend none}
	{tile}
}
