{entity
	(include "/properties/cannon.ext")
	(include "/properties/shadow/tank.inc"  scale(0.6))
	{Extension "52k.mdl"}
	(include "/properties/selection/cannon.inc"  scale(0.7))
	{props "vehicle" "cannon" "heavy" "wheeled" "turret" "turret_rotator" "vision_lev03_can" "art_spec" "texmod_w_cf_cd" "muzzle_brake" "anm_52k"}
	{PatherID "cannon.large"}
	("round_sandbag_flak")	
	{targetSelector "cannon_shell_aa"}		

	{placer	
		{place "gunner" {visor "main"} {useVision 0}}
		{place "commander" {visor "main"} {useVision 0}}
	}	

	{volume "body*"
		{tags "body"}
		{component "body"}
	}
	("v_gun" n(01))
	("v_gun" n(02))
	("v_gun" n(03))
	("v_gun" n(_rot01))
	("v_gun" n(_rot02))
	("v_gun" n(_rot03))
	{volume "turret*"
		{tags "turret"}
		{component "turret"}
	}
	("armor_medium_hp" k(1.1) hp(250))

	("wheel" args "wheelr1")
	("wheel" args "wheelr2")
	("wheel" args "wheell1")
	("wheel" args "wheell2")

	("crew_wheeled_gun")
	("crew_4_supporter_any_set"
		place_l1(charger_l1)
		place_r1(charger_r1)
		place_l2(charger_l2)
		place_r2(charger_r2)
	)
	{boarder
		{anm "gunner"
			{forward	{end "zenit_pointer_right" 10}}
		}
		{anm "commander"
			{forward	{end "cannon_big_commander" 10}}
		}
		{anm "charger_r1"
			{forward	{end "cannon_big_commander" 10}}
		}
		{anm "charger_l1"
			{forward	{end "cannon_big_gunner" 10}}
		}
		{anm "charger_r2"
			{forward {end "artillery_support_r" 10}}
		}
		{anm "charger_l2"
			{forward {end "artillery_support_l" 10}}
		}
		{anm "seat1"
			{forward {end "cannon_crew" 10}}
		}
		{anm "seat2"
			{forward {end "cannon_crew" 10}}
		}
		{anm "seat3"
			{forward {end "cannon_crew" 10}}
		}
		{anm "seat4"
			{forward {end "cannon_crew" 10}}
		}
		{anm "seat5"
			{forward {end "cannon_crew" 10}}
		}
		{anm "seat6"
			{forward {end "cannon_crew" 10}}
		}
		{anm "driver1"
		} 
		{anm "driver2"
		}
		{anm "driver_l2"
		} 
		{anm "driver_r2"
		}
	}
	{extender "inventory"
		{box
			{clear}
			{item "bullet85 ap rus std" 30}
			{item "bullet85 fg rus std" 30}
			{item "repair_kit"}	
			{item "sandbag_kit2"}			
		}
	}
	{Weaponry
		{place "gun"
			{weapon "85mm_52k" filled}
			("abm_field_gun")
		}
	}  	

	{extender "cannon"
		("attach_time")
		{animation
			{aim "cannon_aa_gunlayer_l" "" "" "" "" ""}
			{fire	"cannon_aa_gunlayer_reaction_r" "cannon_big_charger_reaction" "cannon_big_commander_reaction" "cannon_big_charger_reaction" "cannon_big_support_r_reaction" "cannon_big_support_l_reaction"}
			{move "cannon_pak40_pusher_left" "cannon_pak40_pusher_right" "mgun_browning_driver_r" "mgun_browning_driver_l" "mgun_browning_driver_l" "mgun_browning_driver_r"}
			{reload  "" "" "cannon_big_commander_charge" "cannon_big_charger_charge" "cannon_big_support_charge_l" "cannon_big_support_charge_r"}
			{switch "close"}
		}
		{driver {workers "driver1" "driver2"}}
		{move {obstacle "close"} {workers "driver1" "driver2" "driver_l1" "driver_r1" "driver_l2" "driver_r2"}}
		{attack {obstacle "open"} {workers "gunner" "commander" "charger_l1" "charger_r1" "charger_l2" "charger_r2"}}
		{trailing {workers "seat1" "seat2" "seat3" "seat4" "seat5" "seat6"}}
	}
	{Chassis "track"
		("car_trace" fx("tracewheel02") step(0.1) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel02") step(0.1) wheell("wlf1") wheelr("wrf1") bonel("FXtraceL2") boner("FXtraceR2"))
		("car_trace" fx("tracewheel02") step(0.1) wheell("wlf2") wheelr("wrf2") bonel("FXtraceL3") boner("FXtraceR3"))
		("arena_flak_stan_6gunners_carrier")
	}
	{bone "turret"
		{limits}
		{speed 0.035}
	}
	{bone "gun_rot"
		{limits -15 82}	
		{speed 0.01}
	}	
	{extension "sensor.ext"}
	{extension "camouflage.ext"}	
	{mass 4500}

}