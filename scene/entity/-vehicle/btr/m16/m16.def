	(include "/properties/car_crew.ext")
	("bone_rotator")
	
	{targetSelector "armoredcar_hmgun_aa"}
	
	{props "vehicle" "armored_car" "tank" "with_gun" "turret" "turret_rotator" "wheeled" "flak" "toss_aside_the_sides" "vision_lev03" "open_wheelhouse" "art_spec" "ram_lvl2" "texmod_w_cf_cd"}

	("v_body" n(02) t())
	("v_body" n(03) t())
	("v_body" n(04) t())
	("v_body" n(05) t())
	("v_body" n(06) t())
	("v_body" n(07) t())
	("v_body" n(08) t())
	("v_body" n(10) t())
	("v_body" n(11) t())
	("v_body" n(12) t())

	("v_gun" n(01) t())
	("v_gun" n(02) t())
	("v_gun" n(03) t())
	("v_gun" n(04) t())

	("v_gun" n(_rot01) t())
	("v_gun" n(_rot02) t())
	("v_gun" n(_rot03) t())
	("v_gun" n(_rot04) t())
	("v_gun" n(_rot05) t())
	("v_gun" n(_rot06) t())

	{volume "body01"
		{able clear {contact} {contact_ground} {touch} {obstacle}}
	}

	{volume "doorl"
		{tags "body" "body_armor"}
		{component "body"}
	}
	{volume "doorr"
		{tags "body" "body_armor"}
		{component "body"}
	}

	{component "turret"
		{RepairTime 20}
	}
	{component "gun"
		{RepairTime 15}
	}
	{Volume "fuel"
		{thickness 1}
	}
	{volume "sticky"
		{tags "grenade_fg_target"}
		{able clear {contact} {touch}}
		{component "sticky_body"}
	}
	{component "sticky_body"}

	("arena_armor_btr_with_track" turret(6) gun(7) engine(12.5 6) body(12.5 6) track(7) hp(300))
	{armor
		("set_open_roof" name(turret))
		("module_other" t(6))
	}

	{extender "enumerator"
		{group "usa"}
	}

	{extender "inventory"
		{Box
			{clear}
			{item "repair_kit" 2 }
			{item "can" filling "fuel" 60}
			{item "sapper_shovel" 2}
			{item "ammo hmgun" 4400}
			{item "sandbag_kit2"}
		}
	}

	{Weaponry
		{place "gun"
			{RestoreIKAfterAim}
			{foresight "foresight1"}
			{weapon "browning_x4" filled}
			{Barrels 4}
			{LinkAnimation "off"}
			{charger "gunner"}
			("abm_mgun_heavy_plus")
		}
	}

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("tracks")

	{mass 8600}

	{Chassis
		{Locomotion ("locomotion")
			{MaxSpeed	72}
	  		{maxSpeedAtMaxTurn 27}
			{StartTime	2}
			{BrakeTime	1.5}
			{StopTime	2.5}
			{TurnRadius	5}
			{Gears  0.4 0.7 1}
			{RearGears  0.4 0.6}
		}
		("fuelbag"
			volume(281)
			rate(50)
			remain(281)
		)
		{MaxTrailerMass 7350}
		; left track must be first
		("tank_trace" fx("tracks_sm") step(0.3) len(0.35) boneleft("wheell4") boneright("wheelr4"))
		("car_trace" fx("tracewheel01") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
	}
	("chassis_add" speed(72))
	{Extension "m16.mdl"}
    ("animation_resume")
	{extension "crew.ext"}
	{extension "sensor.ext"}
	{extension "camouflage.ext"}
	{bone "turret"
		("arena_turret_speed" args 0.0357)
	}	
	{bone "gun_rot"
		{limits -7 75}
		("arena_turret_speed" args 0.0357)
	}
	(define "support_bones_std"
		("support_bones" n(%n) r(%r) lname(wheel) rname(wheel))
	)
	("support_bones_std" n(1) r(0.48))
	("support_bones_std" n(2) r(0.30))
	("support_bones_std" n(8) r(0.09))
	("support_bones_std" n(3) r(0.17))
	("support_bones_std" n(4) r(0.17))
	("support_bones_std" n(5) r(0.17))
	("support_bones_std" n(6) r(0.17))
	
	{extender "bodykit"
		{"round_sandbag"
			{delay 10}
			{"sandbags_flaktruck" "basis"}		
		}	
	}	
