{ArmoredCar 
	(include "/properties/armoredcar.ext")
	(include "/properties/shadow/tank.inc"  scale(1.0))
	(include "/properties/selection/vehicle.inc"  scale(1.0))
	("bone_rotator")
	{props "vehicle" "armored_car" "tank" "zenite" "turret" "wheeled" "explosion_throw_crew" "vision_lev03" "open_wheelhouse" "with_gun" "ram_lvl1" "texmod_cw_cf_cd"}
	{targetClass "armoredcar" "ap fg bullet"}
	{targetSelector "armoredcar_autocannon"}
	{PatherId "vehicle.small"}

	{extender "enumerator"}
	{extension "crew.ext"}
	{extension "sensor.ext"}
	
	("v_body" n(01) t(1))
	("v_body" n(02) t(2))
	("v_body" n(03) t(3))
	("v_body" n(04) t())
	("v_body" n(05) t(5))
	("v_body" n(09) t(5))
	("v_engine" n(01) t())
	("v_engine" n(02) t())
	("v_gun" n() t())
	("v_gun" n(_rot01) t(1))
	("v_gun" n(_rot02) t(2))
	("v_gun" n(_rot03) t(3))
	("v_gun" n(_rot04) t(2))
	
	{component "turret"
		{RepairTime 20}
	}

	("arena_armor_btr" turret(8) gun(17) engine(8) body(14.5 8) hp(276));must be mantlet 8
	{armor
		("module_body_light"	n(1)	body(8 8 8 14.5))
		("module_body_light"	n(2)	body(14.5 8 8 14.5))
		("module_body_light"	n(3)	body(14.5 8 8 10))
		("module_body_light"	n(5)	body(8))
		("module_gun_light"		n(1)	gun(15))
		("module_gun_light"		n(2)	gun(5))
		("module_gun_light"		n(3)	gun(14))
		("module_other" t(4))
	}

	{extender "inventory"
		{box
			{clear}
			{item "bullet28 apcr ger spzb41" 50}
			{item "repair_kit" 2 }
			{item "can" filling "fuel" 60}
			{item "sapper_shovel" 2}
		}
	}
	{Weaponry
		{place "gun"
			{weapon "28mm_pzb41" filled}
			{RestoreIKAfterAim}
			{charger "gunner"}
			{Rotate}
			("abm_gun")
		}
	}
	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")

	("components_btr" 
		repair_body(20)
		repair_engine(18)
		repair_turret(19)
		repair_gun(11)
		repair_track(7)
		repair_wheel(7)
	)

	{mass 4800}
	{Chassis
		{Locomotion ("locomotion")
			{MaxSpeed	70}	
	  		{maxSpeedAtMaxTurn 35}
			{StartTime	2}
			{BrakeTime	1.5}
			{StopTime	2}
			{TurnRadius	3}
			{TurnTime	1}
			{Gears  0.4 0.7 1}
			{RearGears  0.4 0.7}
		}
		("fuelbag"
			volume(225)
			rate(50)
			remain(225)
		)
		("car_trace" fx("tracewheel02") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel02") step(0.09) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		{MaxTrailerMass 1875}
	}
	("chassis_add" speed(70))
	{Extension "sdkfz221.mdl"}
    ("animation_resume")
	{bone "turret"
		{limits -7 15}
		("arena_turret_speed" args 0.0208)
	}
	{bone "gun_rot"
		{limits -6 25}
		("arena_turret_speed" args 0.0208)
	}
}