		repair_body(20)
		repair_engine(18)
		repair_turret(19)
		repair_gun(11)
		repair_track(7)
		repair_wheel(7)
	)

	{Chassis
		{Locomotion ("locomotion")
			{MaxSpeed	53}
	  		{maxSpeedAtMaxTurn 19.875}	
			{StartTime	3}
			{BrakeTime	1}	
			{StopTime	2.5}
			{TurnRadius	5}
   			{Gears  0.4 0.7 1}
			{RearGears  0.4 0.6}
		}
		("fuelbag"
			volume(281)
			rate(50)
			remain(281)
		)
		{MaxTrailerMass 5000}
		; left track must be first
		("tank_trace" fx("tracks_mid") step(0.5) len(0.285) boneleft("FXtraceL2") boneright("FXtraceR2"))
		("car_trace" fx("tracewheel02") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
	}
	{mass 10000}
	("chassis_add" speed(53))
	
	{bone "turret"
		("arena_turret_speed" args 0.0208)
		{limits}
	}
	{bone "gun_rot"
		{limits -8 40}
	}

	(define "support_bones_std"
