		("set_open_roof" name(turret))
		("module_body_light" n(2) body(14.5))
		("module_other" t(4))
	}


	{extender "inventory"
		{Box
			{clear}
			{item "bullet20 ap ger flak" 300}
			{item "bullet20 fg ger flak" 300}
			{item "ammo mgun" 1000}
			{item "repair_kit" 2 }
			{item "can" filling "fuel" 60}
			{item "sapper_shovel" 2}
			{item "sandbag_kit2"}			
		}
	}
	{Weaponry
		{place "gun"
			{weapon "20mm_flak38" filled}
			{RestoreIKAfterAim}
			{charger "commander"}
			("abm_zenite")			
		}
	}

	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("tracks")

