{Entity
	(include "/properties/tank.ext")
	(include "/properties/shadow/tank.inc"  scale(1.2))
	(include "/properties/selection/vehicle.inc"  scale(1.2))

	{extender "enumerator"
		{digit_folder "red"}
	}
	
	{collider "tank.heavy"}
	{props "heavy" "-turret" "vision_lev05" "tank_hunt" "level06" "ram_lvl8" "texmod_cw_zcw_cf_zcf_cd_zcd_c_zc" "muzzle_brake" "anm_tank"}

	("v_body" n(01) t())
	("v_body" n(02) t(1))
	("v_body" n(03) t(2))
	("v_engine" n(01) t())
	("v_engine" n(02) t(1))
	{volume "gun_rot01"
		{tags "mantlet"}
		{component "mantlet"}
	}
	{volume "gun_rot02"
		{tags "mantlet"}
		{component "mantlet"}
	}

	("arena_armor_mantlet" t(100))
	("arena_armor_spa_heavy" gun(28) engine(40 50 40 25) body(80 50 40 25) track(20) hp(745))
	{armor
		("module_body_medium"	n(1) body(80 40 40 25))
		("module_body_medium"	n(2) body(60 40 40 25))
		("module_engine_medium"	n(1) engine(40 40 40 25))
		("module_detail" n(1) t(4) vol("engine03" "engine04"))
	}

	("shield" args 5)
	{extender "shield"
		{on}
	}
	
	{component "shieldl1" {tags "shield"}}
	{component "shieldl2" {tags "shield"}}
	{component "shieldl3" {tags "shield"}}
	{component "shieldl4" {tags "shield"}}
	{component "shieldl5" {tags "shield"}}
	{component "shieldr1" {tags "shield"}}
	{component "shieldr2" {tags "shield"}}
	{component "shieldr3" {tags "shield"}}
	{component "shieldr4" {tags "shield"}}
	{component "shieldr5" {tags "shield"}}
	
	("crew_4_human")
	("crew_human_single_door" place(charger2) door1(emit0) anm(left) visor(main))
	{boarder
		{anm "driver"
			{forward	{begin	"board_jtiger_driver"} {end "seat_gunner_stand"} {base "open_driver"}}
			{reverse 	{end	"emit_tank_2_hold"}}
		}
		{anm "left"
			{forward	{begin	"board_tank_right"} {end "seat_gunner_stand"}}
			{reverse 	{end	"emit_tank_2_hold"}}
		}
		{anm "right"
			{forward	{begin	"board_tank_left"} {end "seat_gunner_stand"}}
			{reverse 	{end	"emit_tank_2_hold"}}
		}
		{anm "armor"
			{forward	{begin	"board_HighArmor"} 	{end "idle_onboard_l"}}
			{reverse 	{end	"emit_tank_2"}}
		}
		{anm "armor1"
			{forward	{begin	"board_HighArmor"} {end "idle_onboard_r"}}
			{reverse 	{end	"emit_body"}}
		}
	}

    {sensor
        {visor "main"
            {vision "tank_main-h"}
			{bone "gun"}
        }
    }

	("seat_human" place(seat_l1) bone(seat01) door(emit3)  animation(armor))
	("seat_human" place(seat_l2) bone(seat02) door(emit5)  animation(armor))
	("seat_human" place(seat_r1) bone(seat03) door(emit4)  animation(armor1))
	("seat_human" place(seat_r2) bone(seat04) door(emit6)  animation(armor1))

	("components_tank" 
		repair_body(45)
		repair_engine(40)
		repair_turret(40)
		repair_gun(30)
		repair_track(20)
	)
	
	{extender "inventory"
		{box
			{item  "ammo mgun" 2700}
			{item "bullet88 ap ger pak43" 30}
			{item "bullet88 fg ger pak43" 30}
			{item "mg34_vh" filled {user "mgun"}}
		}
	}
	{Weaponry
		("restore_ik_time")
		{place "gun"
			{RestoreIKAfterAim}
			{weapon "88mm_pak43_jagdpanther" filling "ammo bullet88 ap ger pak43" 1}
			{gunner "driver"}
			{charger "driver"}
			("abm_tank_hunt")
			{basic}
		}
		{place "mgun"
			{type "hull"}
			{foresight "foresight3"}
			{gunner "driver"}
			{charger "driver"}
			("abm_mgun")
			{basic}
		}
	}
	{mass 45000}
	{Chassis
		("tank_trace" fx("tracks_big") step(0.47) len(0.46))
		{Locomotion ("locomotion")
			{MaxSpeed	       46}
			{maxSpeedAtMaxTurn 17}
			{StartTime	       4}
			{TurnTime	        8}
			{TurnStart        1.1} 
	    		{BrakeTime        0.5}
     			{StopTime         0.5} 
    			{TurnRadius         6} 
	    		{TurnStop         0.7} 
    			{Gears      0.2 0.5 1.0}
    			{RearGears       0.2 0.45}
		}
		("fuelbag"
			volume(720)
			rate(50)
			remain(720)
		)
	}

	{Extension "jagdpanther.mdl"}
    ("animation_resume")
	{texmod ""}
	
	{bone "mgun"
		{limits 15}
		{speed2 80}
	}
	{bone "gun_rot"
		{limits 20 -5}
	}
	("chassis_add_medium" speed(46))
	{extension "sensor.ext"}
	{extension "camouflage.ext"}
}