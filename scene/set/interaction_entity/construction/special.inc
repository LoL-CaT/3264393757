			{ani_play "hook"}
			{delay 0.4 0.1
				{spawn "metal_hit_fg" "fx_spark"}
			}
			{delay 0.4 0.1
				{spawn "metal_hit_fg" "fx_spark1"}
			}
			{spawn "metal_hit_fg" "fx_spark2"}
			{spawn "metal_hit_fg" "fx_spark3"}
	
			{delay 1
;				{spawn "spherewater_big2" "fx_water_hook"}
				{if game_param 	"biom" "snow"
					{spawn "spheredust_small_w" "fx_water_hook"}
				else
					{spawn "spheredust_small" "fx_water_hook"}
				}
			}
			{delay 5
;				{spawn "spherewater_big2" "fx_waterr3"}
;				{spawn "spherewater_big2" "fx_waterl3"}
				{if game_param 	"biom" "snow"
					{spawn "spheredust_small_w" "fx_waterr3"}
					{spawn "spheredust_small_w" "fx_waterr3"}
				else
					{spawn "spheredust_small" "fx_waterr3"}
					{spawn "spheredust_small" "fx_waterr3"}
				}
;				{delay 0.1
;					{spawn "spherewater_big2" "fx_waterr2"}
;					{spawn "spherewater_big2" "fx_waterl2"}
;				}
;				{delay 0.2
;					{spawn "spherewater_big2" "fx_waterr1"}
;					{spawn "spherewater_big2" "fx_waterl1"}
;				}
			}
		}
	}
	{on animation_end "piece"
		{delay 0
			{spawn "metal_hit_fg" "fx_spark"}
			{spawn "metal_hit_fg" "fx_spark1"}
			(define "cut_piece"
				{tear "piece_large_steel" bone "piece%0"
					{volumes enable contact}
					{impulse up -5 no_position}
				}
			)
			("cut_piece" args 1)
		}
	}
}

{"sand sandbag heap"
	{##on blast
		{if volume "body"
			{if min_energy 4.7 ;7
				{call "stage2"}
			else min_energy 4 ;0.5
				{if rand 0.25
					{call "stage1"}
				else
					{call "stage2"}
				}
			else min_energy 3 ;0.5
				{if rand 0.6
					{call "stage1"}
				else
					{call "stage2"}
				}
			else min_energy 1.6 ;0.5
				{if rand 0.4
					{call "stage1"}
				else rand 0.4
					{call "stage2"}
				}
			}
		}
	}
	{on "break"
		{call "stage2"}
	}
	{on break_armor
		 {if volume "body"
		 	{call "stage2"}
		 }
	}
	{on contact
		{if can_crush
			{volumes disable contact}
			{if not flag "stage1"
				{call "stage1"}
			else rand 0.5
				{delay 0.5
					{call "stage2"}
				}
			}
		else effector "air_part"
			{if min_speed 5
				{call "stage2"}
			}
		}
	}
	{on bullet_hit
		{if stuff "shell"
			{if stuff "fg" or stuff "fr"
				{if stuff "big"
					{call "stage2"}
				}
			}
		}
	}
	{on "stage1"
		{if not flag "stage1"
			{set "stage1" 1}
;			{volumes disable contact}
			{call "change_model"}
			{delay 0.01
				{volumes enable contact visible}
			}
			{delay 0.1
				{bone show "x1"}
				{bone hide "x2"}
				{ani_play "crush1"}
				{delay 0.1
					{update_visibility_patch}
				}
			}
		}
	}
	{on "stage2"
		{if not flag "stage2"
			{set "stage1" 1}
			{set "stage2" 1}
			{call "change_model"}
			{tags add "crushed"}
			{delay_ex 0.1
				{bone hide "x1"}
				{bone show "x2"}
				{ani_play "crush2" 1}
				{volumes "body" disable bullet contact blast visible touch}
				{call "update_fx"}
				{set "first_stage" 1}
				{set "last_stage" 1}
				{call "effects"}
				{delay_ex 0.1
					{call "cut_pieces"}
				}
				{delay 0.3
					{update_visibility_patch}
					{area disable}
				}
				{land off}
			}
		}
	}
	{on "change_model"
		{if not flag "sliced"
			{set "sliced" 1}
			{delay 0
				{set_skeleton "_x"}
				{volumes disable contact visible}
			}
		}
	}
	{on "cut_pieces"
		(define "cut_piece"
			{tear "piece_medium_sand" bone "piece%0"
				{volumes enable contact}
				{impulse up 5 2 dir 3.5 2.5 rnd_dir 5 no_position}
				{if rand 0.3
					{add_view "dust_small" "dust" "root"}
					{view start "dust"}
					{delay_effect 2 1 "stop"}
				}
			}
		)
		("cut_pieces")
	}
	{on spawn
		{if game_param 	"biom" "snow"
			{tex_mod "win"}
		}
	}
}

