(include "tps.inc")
{parameters
	{range_degrees "camera_pitch_min" 	-20	-30}
	{range "camera_offset_up"		   3.4	3.4}   ; точка прицела относительно крыши машины
	{range "camera_distance"		 0.1	12}     ;отдаление камеры от юнита
	{range "camera_offset_forward"    	0 -1}  ;смещение вперЄд точки внимани¤
	{visual_set "visual_set" "tps_autocannon"}
}
{suitable "sdkfz232 tps" {priority 15}}
