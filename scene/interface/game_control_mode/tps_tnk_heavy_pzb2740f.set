(include "tps.inc")
{parameters
	{range "camera_offset_up"		3.4	3.4}   ; точка прицела относительно крыши машины
	{range "camera_distance"		0.1	12.5}    ;отдаление камеры от юнита
	{range "camera_offset_forward" 	0 -1}      ;смещение вперёд точки внимания
	{visual_set "visual_set" "tps_tank"}
}
{suitable "pzb2740f tps" {priority 15}}
