(include "tps.inc")
{parameters
	{range "camera_offset_up"		3.3	3.3}    	; точка прицела относительно крыши машины
	{range "camera_distance"		1	14}	    ; отдаление камеры от юнита
	{range "camera_offset_forward" 	0.5 0.9}	    ; смещение вперёд точки внимания
	{visual_set "visual_set" "tps_tank"}
}
{suitable "mk2a tps" {priority 15}}
