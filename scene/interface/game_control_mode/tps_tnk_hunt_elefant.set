(include "tps.inc")
{parameters
	{range "camera_offset_up"		3.8	3.8}     ; точка прицела относительно крыши машины
	{range "camera_distance"		0.1	14}      ;отдаление камеры от юнита
	{range "camera_offset_forward"  -1 -1}       ;смещение вперёд точки внимания
	{visual_set "visual_set" "tps_tank"}
}
{suitable "elefant tps" {priority 15}}
