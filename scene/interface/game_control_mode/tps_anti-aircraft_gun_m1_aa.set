(include "tps.inc")
{parameters
	{range_degrees "camera_pitch_min" 		-10	-10}
	{range_degrees "camera_pitch_max"		75	2}
	{range "camera_offset_up"			 	2.2 2.2}      ;точка прицела относительно крыши машины
	{range "camera_distance"		 		0.1 9}        ;отдаление камеры от юнита
	{range "camera_offset_forward"        	-2 -1}        ;смещение вперед точки внимания
	{visual_set "visual_set" "tps_tank"}
}
{suitable "m1_aa tps" {priority 15}}
