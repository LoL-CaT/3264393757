(include "tps.inc")
{parameters
	{range "camera_offset_up"		4.5	4.5}	;точка прицела относительно крыши машины
	{range "camera_distance"		0.1	15}		;отдаление камеры от юнита
	{range "camera_offset_forward" 	-1 -1}		;смещение вперёд точки внимания
	{visual_set "visual_set" "tps_tank"}
}
{suitable "m4a3dd_inf tps" {priority 15}}
