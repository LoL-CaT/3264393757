(include "tps.inc")
{switch_to_time 0.25}
{parameters
	{range "camera_offset_up"		 2.5	5}   ; точка прицела относительно крыши машины
	{range "camera_distance"		 5	20}     ;отдаление камеры от юнита
	{range "camera_offset_forward" 		5	0}	; Смещение точки внимания по продольной оси	
	{range_degrees "camera_fov"		60	60}	
	{visual_set "visual_set" "tps_tank"}	
}
{suitable "air-bag tps" {priority 15}}


