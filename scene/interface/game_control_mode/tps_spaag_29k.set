(include "tps.inc")
{parameters
	{range_degrees "camera_pitch_min" 		-10	-10}
	{range_degrees "camera_pitch_max"		75	2}
	{range "camera_offset_up"		   3.5	3.4}   ; точка прицела относительно крыши машины
	{range "camera_distance"		 0.1	14}     ;отдаление камеры от юнита
	{range "camera_offset_forward"    	-1 -1}  ;смещение вперёд точки внимания
	{visual_set "visual_set" "tps_autocannon"}	
}
{suitable "29k tps" 	{priority 15}}						